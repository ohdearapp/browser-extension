{
  "manifest_version": 2,
  "name": "Oh Dear!",
  "version": "1.0.0",
  "description": "SSL/TLS certificate health and availability check for any website.",
  "icons": {
    "128": "icons/icon128.png"
  },
  "homepage_url": "https://ohdear.app",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "browser_style": true,
    "default_title": "Oh Dear!",
    "default_popup": "popup/window.html"
  }
}
